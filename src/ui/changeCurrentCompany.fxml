<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="Content" minHeight="-1.0" minWidth="-1.0" prefHeight="545.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="-1.0" layoutY="-2.0" />
      <Label layoutX="130.0" layoutY="70.0" text="Cambiar empresa actual">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="searchParameterCompany" layoutX="103.0" layoutY="231.0" promptText="Ingrese NIT" />
      <Button layoutX="274.0" layoutY="231.0" mnemonicParsing="false" onAction="#searchCompany" text="Buscar" />
      <Label layoutX="88.0" layoutY="144.0" prefHeight="72.0" prefWidth="264.0" text="Esta opcion permite cambiar la empresa sobre la cual los demas procesos operan." textAlignment="CENTER" textFill="#000000b2" wrapText="true" />
      <Label fx:id="infoFoundCompanyLabel" disable="true" layoutX="140.0" layoutY="273.0" text="Empresa no encontrada!" textAlignment="CENTER" visible="false" />
      <Button fx:id="changeCompanyButton" disable="true" layoutX="144.0" layoutY="424.0" mnemonicParsing="false" onAction="#runChangeCurrentCompany" prefHeight="40.0" prefWidth="155.0" text="Confirmar cambio">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
